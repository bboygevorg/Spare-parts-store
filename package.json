{
  "name": "buildclub-pwa",
  "version": "1.0.0",
  "description": "Tour Armenia PWA",
  "main": "server.js",
  "scripts": {
    "dev:build-client": "webpack --config webpack.client.dev.js  --watch",
    "dev:build-server": "webpack --config webpack.server.js  --watch",
    "dev:run-server": "nodemon build/bundle.js",
    "start-dev": " env-cmd -f ./dev.env npm-run-all --parallel dev:*",
    "staging:build-client": "node ./update-build.js && env-cmd -f ./staging.env webpack -p --mode=production --config webpack.client.prod.js",
    "staging:build-server": "env-cmd -f ./staging.env webpack --config webpack.server.js",
    "staging:run-server": "node build/bundle.js",
    "start-staging": "env-cmd -f ./staging.env npm-run-all staging:*",
    "prod:build-client": "node ./update-build.js && env-cmd -f ./prod.env webpack -p --mode=production --config webpack.client.prod.js",
    "prod:build-server": "env-cmd -f ./prod.env webpack --config webpack.server.js",
    "prod:run-server": "node build/bundle.js",
    "start-prod": "env-cmd -f ./prod.env npm-run-all prod:*",
    "start-forever": "env-cmd -f ./prod.env node build/bundle.js &",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "npm-run-all webpack:build-*"
  },
